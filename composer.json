{
  "name": "maxim/easy-bundle",
  "version": "0.0.2",
  "type": "symfony-bundle",
  "description": "Symfony easy bundle",
  "lisence": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "4.4.*",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/yaml": "4.4.*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Maxim\\EasyBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maxim\\EasyBundle\\Tests": "Tests/"
    }
  }
}