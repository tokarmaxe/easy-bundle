{
  "name": "maxim/easy-bundle",
  "version": "0.0.2",
  "type": "symfony-bundle",
  "description": "Symfony easy bundle",
  "lisence": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-gearman": "*",
    "symfony/form": "^3.4|^4.0",
    "symfony/framework-bundle": "^3.4|^4.0",
    "symfony/twig-bundle": "^3.4|^4.0",
    "symfony/yaml": "4.4.*"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "Maxim\\EasyBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maxim\\EasyBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "ext-pcntl": "*",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.0",
    "mockery/mockery": "^1.5",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^3.4|^4.0",
    "symfony/dotenv": "^3.4|^4.0",
    "symfony/flex": "^1.19",
    "symfony/phpunit-bridge": "^6.2",
    "symfony/process": "^5.4",
    "symfony/templating": "^3.4|^4.0"
  }
}
